{
  "name": "simple-ts-react-app",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://sophiabrandt.github.io/typescript-react-cocktails",
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.3.0",
    "typescript": "3.7.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "ts": "tsc",
    "dev": "concurrently -k \"npm run ts\" \"npm run start\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "24.0.23",
    "@types/node": "12.12.19",
    "@types/react": "16.9.16",
    "@types/react-dom": "16.9.4",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "concurrently": "^5.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "gh-pages": "^2.1.1",
    "prettier": "^1.19.1"
  }
